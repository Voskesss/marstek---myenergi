# myenergi-marstek Configuration Example
# Copy to .env and adjust values

# myenergi API
MYENERGI_BASE_URL=https://s18.myenergi.net
MYENERGI_HUB_SERIAL=Z12345678
MYENERGI_API_KEY=your_api_key_here

# Marstek Battery
MARSTEK_BASE_URL=http://192.168.68.66:30000
MARSTEK_API_TOKEN=
MARSTEK_USE_BLE=true

# myenergi Priority Control (Zappi > Eddi > Battery)
EDDI_PRIORITY_MODE=threshold
EDDI_ACTIVE_W=200
EDDI_MAX_CAPACITY_W=3600
EDDI_RESERVE_W=3000
ZAPPI_ACTIVE_W=200
ZAPPI_RESERVE_W=2000
BATTERY_MIN_EXPORT_W=5000
BATTERY_HYSTERESIS_W=500
EDDI_TARGET_TEMP_1=59
EDDI_TARGET_TEMP_2=59
EDDI_TEMP_HYSTERESIS=3
EDDI_USE_TANK_1=true
EDDI_USE_TANK_2=false

# Control Logic
EXPORT_ENOUGH_W=300
IMPORT_DIP_W=150
STABLE_EXPORT_SECONDS=30
MIN_SWITCH_COOLDOWN_S=60
SOC_FAILSAFE_MIN=15
POLL_INTERVAL_S=2
